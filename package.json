    "jsonwebtoken": "^9.0.2",
    "next": "^13.5.6",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stripe": "^14.12.0"
  },
  "devDependencies": {
