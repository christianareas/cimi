{
  "name": "cimi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "reinstall": "rm -rf node_modules && rm -rf .next && rm package-lock.json && npm i",
    "update:ts": "npm i -D typescript@latest @types/node@latest @types/react@latest @types/react-dom@latest",
    "update:next": "npm i next@latest react@latest react-dom@latest",
    "update:tailwind": "npm i -D tailwindcss@latest @tailwindcss/postcss@latest autoprefixer@latest",
    "update:radix": "npm i @radix-ui/react-dialog@latest @radix-ui/react-form@latest @radix-ui/react-icons@latest @radix-ui/react-select@latest @radix-ui/react-tabs@latest @radix-ui/react-visually-hidden@latest",
    "update:markdown": "npm i react-markdown@latest rehype-raw@latest remark-gfm@latest",
    "update:eslint": "npm i -D eslint@latest eslint-config-next@latest",
    "update:biome": "npm i -D @biomejs/biome@latest",
    "update:all-dependencies": "npm run update:ts && npm run update:next && npm run update:tailwind && npm run update:radix && npm run update:markdown && npm run update:eslint && npm run update:biome && npm ls && npm outdated"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-form": "^0.1.1",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-select": "^2.1.5",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-visually-hidden": "^1.1.1",
    "next": "^15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.3",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tailwindcss/postcss": "^4.0.3",
    "@types/node": "^22.13.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.19.0",
    "eslint-config-next": "^15.1.6",
    "tailwindcss": "^4.0.3",
    "typescript": "^5.7.3"
  }
}
