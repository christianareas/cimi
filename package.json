  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "update-ts": "npm i -D typescript@latest @builder.io/react@latest @types/bcryptjs@latest @types/jsonwebtoken@latest @types/next-auth@latest @types/node@latest @types/react@latest @types/react-dom@latest",
    "update-next": "npm i next@13.x react@latest react-dom@latest",
    "update-prisma": "npm i prisma@latest @prisma/client@latest",
    "update-auth": "npm i bcryptjs@latest jsonwebtoken@latest next-auth@latest",
    "update-stripe": "npm i stripe@latest @stripe/stripe-js@latest @stripe/react-stripe-js@latest",
    "update-builder": "npm i -D @builder.io/react@latest",
    "update-tailwind": "npm i -D tailwindcss@latest postcss@latest autoprefixer@latest",
    "update-eslint": "npm i -D eslint@latest eslint-config-next@latest",
    "update-all": "npm run update-ts && npm run update-next && npm run update-prisma && npm run update-auth && npm run update-stripe && npm run update-builder && npm run update-tailwind && npm run update-eslint"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.3.0",
