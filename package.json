{
  "name": "cimi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "update:ts": "npm i -D typescript@latest @types/bcryptjs@latest @types/jsonwebtoken@latest @types/node@latest @types/react@latest @types/react-dom@latest",
    "update:next": "npm i next@latest react@latest react-dom@latest",
    "update:prisma": "npm i prisma@latest @prisma/client@latest",
    "update:stripe": "npm i stripe@latest @stripe/stripe-js@latest @stripe/react-stripe-js@latest",
    "update:tailwind": "npm i -D tailwindcss@latest postcss@latest autoprefixer@latest",
    "update:radix": "npm i @radix-ui/react-dialog@latest @radix-ui/react-form@latest @radix-ui/react-icons@latest @radix-ui/react-visually-hidden@latest",
    "update:remark": "npm i remark@latest remark-html@latest",
    "update:eslint": "npm i -D eslint@^8.x eslint-config-next@latest",
    "update:biome": "npm i -D @biomejs/biome@latest",
    "update:all-dependencies": "npm run update:ts && npm run update:next && npm run update:prisma && npm run update:stripe && npm run update:tailwind && npm run update:radix && npm run update:remark && npm run update:eslint && npm run update:biome && npm ls && npm outdated"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-form": "^0.1.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-visually-hidden": "^1.1.0",
    "@stripe/react-stripe-js": "^2.7.3",
    "@stripe/stripe-js": "^4.1.0",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "stripe": "^16.6.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "postcss": "^8.4.40",
    "prisma": "^5.17.0",
    "tailwindcss": "^3.4.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.4"
  }
}
